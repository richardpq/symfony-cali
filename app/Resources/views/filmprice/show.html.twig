{% extends 'base.html.twig' %}

{% block body %}
    <h1>FilmPrice</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ filmPrice.id }}</td>
            </tr>
            <tr>
                <th>Sqftprice</th>
                <td>{{ filmPrice.sqftPrice }}</td>
            </tr>
            <tr>
                <th>Frenchpanesprice</th>
                <td>{{ filmPrice.frenchPanesPrice }}</td>
            </tr>
            <tr>
                <th>Startdate</th>
                <td>{% if filmPrice.startDate %}{{ filmPrice.startDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Enddate</th>
                <td>{% if filmPrice.endDate %}{{ filmPrice.endDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('filmprice_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('filmprice_edit', { 'id': filmPrice.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
