{% set entityName = 'Film Roll' %}
{% embed 'RichardPQAdminBundle:layout:crud.new_edit.html.twig' with {
    'form':form,
    'submit_name':'Create',
    'entity_route_prefix':'film-roll-inventory',
    'form_title': 'Add New '~entityName
}%}

    {% block content_header_title %}Film Roll{% endblock %}
    {% block content_header_title_small %}New{% endblock %}

    {% block content_header_breadcrumb %}Inventory{% endblock %}
    {% block content_header_breadcrumb_2lv %}Add/Edit{% endblock %}
    {% block breadcrumb_icon 'pencil' %}
    {% block pageId 'filmrollcrud' %}
    {% block custom_stylesheets %}
        <!-- Select2 -->
        <link rel="stylesheet" href="{{ asset('bundles/richardpqadmin/plugins/select2/select2.min.css') }}">
    {% endblock %}
    {% block custom_jscripts %}
        <!-- Select2 -->
        <script src="{{ asset('bundles/richardpqadmin/plugins/select2/select2.full.min.js') }}"></script>
        <script>
            //Initialize Select2 Elements
            $(".select2").select2();
        </script>

    {% endblock %}
{% endembed %}